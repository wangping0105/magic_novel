<%if flash.present? %>
    <% flash.each do |key, value| %>
        <div class="alert alert-<%= key %>" role="alert">
          <a href="#" class="close" data-dismiss="alert">&times;</a>
         <%= value %>
        </div>
    <% end %>
<%end %>

<div id="faye_msg" class="alert_msg faye_msg">
</div>
